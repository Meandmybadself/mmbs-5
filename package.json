{
  "name": "mmbs-5",
  "version": "1.1.0",
  "description": "Me & My Bad Self - Mk. V",
  "scripts": {
    "clean": "rimraf dist .twelvety.cache",
    "clear": "npm run clean",
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:server": "cross-env ELEVENTY_ENV=production OUTPUT_DIR=stage eleventy",
    "watch": "cross-env ELEVENTY_ENV=development eleventy --watch",
    "serve": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev": "npm run serve",
    "promote": "ssh mn 'cd /var/www/static-sites/meandmybadself.com && npm run build:server && rm -rf dist && mv stage dist && cp -R src/_assets/ dist/_assets/"
  },
  "repository": "https://github.com/meandmybadself/mmbs-5",
  "author": "Jeffery Bennett",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.3",
    "browserify": "^17.0.0",
    "clean-css": "^5.3.3",
    "critical": "^7.2.1",
    "cross-env": "^7.0.3",
    "deasync": "^0.1.21",
    "html-minifier": "^4.0.0",
    "instant.page": "^5.1.0",
    "js-beautify": "^1.13.1",
    "module-alias": "^2.2.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss": "^8.2.4",
    "postcss-preset-env": "^10.2.4",
    "punycode": "^2.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.32.5",
    "sharp": "^0.34.2",
    "tinyify": "^4.0.0",
    "uglify-js": "^3.12.5"
  }
}
