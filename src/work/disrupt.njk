{% stylesheet 'scss' %}
  $accent: #00bf5c;
  @import 'global';
  @import 'work';
  .grid--disrupt {
    .box {
      min-height:350px;
      &:nth-child(3) {
        grid-area: 4/1/5/3;
      }
      &:nth-child(4) {
        grid-area: 4/3/5/5;
      }
      &:nth-child(5) {
        grid-area: 5/1/6/3;
      }
      &:nth-child(6) {
        grid-area: 5/3/6/5;
      }
      &:nth-child(7) {
        grid-area: 6/1/7/3;
      }
      &:nth-child(8) {
        grid-area: 6/3/7/5;
      }
    }

  }
{% endstylesheet %}